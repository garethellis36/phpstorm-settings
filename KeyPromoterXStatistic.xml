<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Start Listening for PHP Debug Connections">
        <Item>
          <IdeaActionID>PhpListenDebugAction</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+F9'</ShortCut>
          <Description>Start Listening for PHP Debug Connections</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Commit">
        <Item>
          <IdeaActionID>Console.Transaction.Commit</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+Enter'</ShortCut>
          <Description>Commit</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Data Source Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'Shift+Enter'</ShortCut>
          <Description>Data Source Properties</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Console...">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.Any</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Jump to Console...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop Listening for PHP Debug Connections">
        <Item>
          <IdeaActionID>PhpListenDebugAction</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+F9'</ShortCut>
          <Description>Stop Listening for PHP Debug Connections</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>DatabaseView.SynchronizeAction</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>